
<%=render "dangqun_left"%>

<div id="main-wrapper">
	<div id="main-content">
		<%=render "shared/notice"%>
        <div class="title">
			<h3>廉洁从政管理</h3>
		</div>
     <div class="hastable">
     		<table id="datatable" class="display"> 
						<thead> 
						<tr>
							<th>#ID</th>
				
						    <th>标题</th> 			    						      
						    <th>操作</th> 
							
						</tr> 
						</thead> 
						<tbody> 
	
					   <% @cms_dangquns.each do |cms_dangqun| %>
     					<tr>
        					<td><%=cms_dangqun.id %></td>
        		
        				
        					<td><%=cms_dangqun.title %></td>
        				
        					<td><%=link_to "下载",download_cms_dangqun_path(cms_dangqun),class: "btn_no_text btn ui-state-default ui-corner-all tooltip"%>
        						<%=link_to "编辑",edit_cms_dangqun_path(cms_dangqun),class: "btn_no_text btn ui-state-default ui-corner-all tooltip"%>
        						<%=link_to "删除",cms_dangqun, method: :delete, data: { confirm: 'Are you sure?' },class: "btn_no_text btn ui-state-default ui-corner-all tooltip" %>
        					
          					</td>  				
      					</tr>
    					<% end %>
				
						</tbody>
						</table>



</div>
</div>


